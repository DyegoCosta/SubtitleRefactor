<!DOCTYPE html>
<html>
<head>
    <title>SubRefactor - @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/redmond/jquery-ui-1.8.14.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jQuery/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQuery/jquery-ui-1.8.12.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQuery/jquery.ui.core.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQuery/jquery.ui.widget.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQuery/jquery.ui.position.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQuery/jquery.ui.dialog.min.js")" type="text/javascript"></script>    
    <script src="@Url.Content("~/Scripts/jQuery/jquery.ui.button.min.js")" type="text/javascript"></script> 
</head>
<body>
    <div class="page">
        <div id="header">
            <div class="banner">
            </div>
            <div class="menu-container">
                <ul id="menu">
                    <li><a href="@Url.Content("~/Subtitle/Synchronize")">
                        <img src="../../Content/themes/base/images/sync-button-icon.png" />
                    </a></li>
                    <li><a href="@Url.Content("~/Subtitle/Translate")">
                        <img src="../../Content/themes/base/images/translate-button-icon.png" />
                    </a></li>
                    <li><a href="@Url.Content("~/Subtitle/Edit")">
                        <img src="../../Content/themes/base/images/edit-button-icon.png" />
                    </a></li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
            @Html.ActionLink("Home", "Index", "Home") - @Html.ActionLink("About", "About", "Home")
        </div>
    </div>
</body>
</html>
